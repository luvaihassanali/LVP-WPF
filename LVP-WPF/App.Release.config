<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations see the web.comfig examples at http://go.microsoft.com/fwlink/?LinkId=214134\. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="CartoonLimit" value="25" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		<add key="Drives" value="D;E" xdt:Transform="Replace" xdt:Locator="Match(key)"/>=
		<add key="Esp8226Enabled" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		<add key="Esp8226HideCursor" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		<add key="LibreTranslatePath" value="%LOCALAPPDATA%\Programs\Python\Python39\Scripts\" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		<add key="LoggingEnabled" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		<add key="SerialPort" value="3" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		<add key="SerialPortEnabled" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
	</appSettings>
</configuration>